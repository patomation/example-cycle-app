    "type": "git",
    "url": "git+https://github.com/mightyiam/example-cycle-app.git"
  },
  "author": "Shahar Or <mightyiampresence@gmail.com> (https://mightyi.am/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mightyiam/example-cycle-app/issues"
  },
  "homepage": "https://github.com/mightyiam/example-cycle-app#readme",
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@commitlint/travis-cli": "8.2.0",
    "@cycle/dom": "22.4.0",
    "@cycle/run": "5.4.0",
    "@cycle/time": "0.20.0",
    "@typescript-eslint/eslint-plugin": "2.8.0",
    "ava": "2.4.0",
    "eslint": "6.6.0",
    "eslint-config-standard-with-typescript": "11.0.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "esm": "3.2.25",
    "husky": "3.1.0",
    "npm-run-all": "4.1.5",
    "ts-loader": "6.2.1",
    "ts-node": "8.5.2",
    "tsconfigs": "4.0.1",
    "typescript": "3.7.2",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0",
    "xstream": "11.11.0"
  },
  "dependencies": {},
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "esm",
      "ts-node/register/transpile-only"
    ]
  }
